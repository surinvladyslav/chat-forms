{
    "env": {
      "node": true,
      "commonjs": true,
      "browser": true,
      "es6": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "react/prop-types": "off",
        "no-unused-vars": "off",
        "react/no-unknown-property": "off",
        "react/no-unescaped-entities": 0,
        "react/react-in-jsx-scope": "off",
        "semi": "off",
        "@typescript-eslint/semi": "off",
        "react/display-name": "off"
    }
}
